{
  "name": "@emcasa/storybook-native",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.7.8",
  "private": true,
  "scripts": {
    "dev": "bash -c 'concurrently \"yarn expo --dev --no-minify\" \"yarn storybook\"'",
    "expo": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "storybook": "storybook start -p 7007",
    "update-version": "sed -i.bak -E \"s/(\\\"version\\\":) \\\"(.*)\\\"/\\1 \\\"$(project-version)\\\"/\" app.json",
    "publish-expo": "./scripts/publish.sh"
  },
  "dependencies": {
    "@emcasa/ui": "^0.7.6",
    "@emcasa/ui-native": "^0.7.8",
    "expo": "31.0.5",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-svg": "^8.0.8",
    "schedule": "0.4.0",
    "styled-components": "^3.4.10",
    "styled-system": "^3.1.6",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-links": "^4.0.7",
    "@storybook/addons": "^4.0.7",
    "@storybook/react-native": "^4.0.7",
    "babel-preset-expo": "^5.0.0",
    "commit-status": "^4.3.0",
    "concurrently": "^4.0.1",
    "expo-cli": "^2.4.0",
    "get-yarn-workspaces": "^1.0.2",
    "project-version": "^1.2.0",
    "react-dom": "^16.5.2"
  }
}
